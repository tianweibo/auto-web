{
  "private": true,
  "scripts": {
    "dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "testing": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-descriptions": "^1.6.6",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.30.8",
    "@umijs/preset-react": "1.x",
    "antd": "^4.12.3",
    "array-move": "^3.0.1",
    "buried-points-sdk-all": "git+ssh://git@git.dev.enbrands.com:fe-construction/buried-points-sdk-all.git",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "echarts": "^5.1.2",
    "fetch-jsonp": "^1.1.3",
    "moment": "^2.29.1",
    "react-sortable-hoc": "^2.0.0",
    "umi": "^3.3.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/test": "^3.3.7",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
